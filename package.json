{
  "name": "iguploader",
  "productName": "iguploader",
  "version": "0.1.1",
  "description": "Instagram uploader",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "env 'DEBUG=electron-forge:*' electron-forge package",
    "make": "env 'DEBUG=electron-forge:*' electron-forge make",
    "publish": "env 'DEBUG=electron-forge:*' electron-forge publish",
    "lint": "eslint --ext .ts .",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r ts-node/register 'tests/**/*.ts'",
    "generate-barrels": "barrelsby --delete --directory ./src -l below -V"
  },
  "keywords": [],
  "author": {
    "name": "Michele Cantelli",
    "email": "emmekappa@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "iguploader"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "emmekappa",
              "name": "iguploader"
            },
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-dmg": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.52",
    "@electron-forge/publisher-github": "^6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/chai": "^4.2.12",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mocha": "^8.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "barrelsby": "^2.2.0",
    "chai": "^4.2.0",
    "css-loader": "^4.2.1",
    "electron": "^9.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "mocha": "^8.1.1",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.12.21",
    "spectron": "^11.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "clsx": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "instagram-private-api": "^1.41.0",
    "jimp": "^0.16.0",
    "material-ui-dropzone": "^3.3.1",
    "notistack": "^0.9.17",
    "react": "^16.13.1",
    "react-disable": "^0.1.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
